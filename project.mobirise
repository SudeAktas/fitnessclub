              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".main-row": {
              "& when (@reverseContent)": {
                "-webkit-flex-direction": "row-reverse",
                "flex-direction": "row-reverse"
              }
            },
            "h5": {
              "letter-spacing": "0px",
              "margin": "0",
              "width": "100%"
            },
            ".text": {
              "letter-spacing": "3px",
              "color": "#b9b9b9",
              "border-bottom": "1px solid @borderColor",
              "padding-bottom": "15px"
            },
            "amp-img": {
              "margin": "auto",
              "margin-bottom": "0rem",
              "width": "~\"@{logoWidth}px\""
            },
            ".img-wrap": {
              "width": "80px",
              "padding-bottom": "1rem"
            },
            ".btn": {
              "margin-top": "1.5rem"
            },
            ".area": {
              "width": "100%"
            },
            "textarea": {
              "width": "100%",
              "color": "white",
              "background-color": "#212121",
              "border": "none"
            },
            "input": {
              "margin-bottom": "1rem",
              "width": "100%",
              "color": "white",
              "background-color": "#212121",
              "border": "none"
            },
            ".items": {
              "padding-bottom": "10px"
            },
            ".list-item": {
              "display": "-webkit-flex",
              "padding-bottom": "15px",
              "padding-top": "0px"
            },
            ".contacts-ico": {
              ".iconfont-wrapper": {
                "display": "inline-block",
                "width": "~\"@{iconsSizeCont}rem\"",
                "height": "~\"@{iconsSizeCont}rem\"",
                "margin": "auto",
                "text-align": "center",
                "margin-right": "1rem",
                ".mbr-iconfont-c": {
                  "color": "@iconsColorCont",
                  "font-size": "~\"@{iconsSizeCont}rem\""
                }
              },
              "display": "-webkit-flex",
              "margin-right": "50px",
              "padding-bottom": "20px",
              "color": "#b9b9b9"
            },
            ".contacts": {
              "margin": "auto"
            },
            ".sub-title": {
              "color": "#b9b9b9"
            },
            ".text2": {
              "color": "#b9b9b9"
            },
            ".mbr-editable-button .btn-wrap": {
              "display": "flex",
              "-webkit-align-items": "center",
              "align-items": "center"
            },
            "@media (max-width: 991px)": {
              ".padding": {
                "margin-bottom": "2rem"
              }
            },
            ".text3": {
              "color": "#dadada"
            }
          },
          "_name": "footer2",
          "_customHTML": "<section class=\"footer2\" group=\"Footers\" plugins=\"amp-main,amp-form,amp-mustache,amp-iframe\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n\n    <mbr-parameters>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"2\" condition=\"fullScreen == false\">\n        <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\">\n        <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked=\"\">\n        <input type=\"color\" title=\"Border Color\" name=\"borderColor\" value=\"#828282\">\n        <input type=\"range\" title=\"Logo Width\" inline=\"\" name=\"logoWidth\" min=\"50\" max=\"120\" value=\"88\" step=\"1\">\n        <input type=\"color\" title=\"Contacts Icons Color\" name=\"iconsColorCont\" value=\"#b9b9b9\" condition=\"showIcons\">\n        <input type=\"range\" inline=\"\" title=\"Contacts Icons Size\" name=\"iconsSizeCont\" min=\"0.2\" max=\"2\" step=\"0.1\" value=\"1.4\" condition=\"showIcons\">\n        <input type=\"range\" inline=\"\" name=\"listItems\" title=\"List Items\" min=\"0\" max=\"6\" value=\"6\">\n        <input type=\"checkbox\" title=\"Form on Left/Right\" name=\"reverseContent\">\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#29282d\" selected=\"\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"container\">\n\n        <div class=\"mbr-row mbr-justify-content-center main-row\">\n            <div class=\"mbr-col-lg-3 mbr-col-sm-12 padding\">\n                <h3 class=\"mbr-text mbr-bold mbr-fonts-style group-title mbr-white\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".group-title\">FITNESSAMP</h3>\n                <div class=\"img-wrap\" mbr-if=\"showImg\">\n                    <amp-img src=\"../_images/logo.png\" layout=\"responsive\" width=\"90.5311778290993\" height=\"80\" alt=\"Mobirise\" class=\"mobirise-loader\">\n                        <div placeholder=\"\" class=\"placeholder\"></div>\n                        <img class=\"mbr-temp\" src=\"../_images/logo.png\" alt=\"Mobirise\">\n                    </amp-img>\n                </div>\n                <p class=\"mbr-fonts-style text3\" mbr-theme-style=\"display-5\" data-app-selector=\".text3\" mbr-if=\"showText\">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Sed vehicula blandit mi. Maecenas elementum tincidunt.</p>\n\n\n                <div class=\"contacts-ico\">\n                    <span class=\"iconfont-wrapper\">\n                        <span mbr-icon=\"\" class=\"mbr-iconfont-c mbr-iconfont fa-tablet fa\" mbr-if=\"showIcons\"></span>\n                    </span>\n                    <h5 class=\"mbr-fonts-style contacts\" mbr-theme-style=\"display-5\" data-app-selector=\".contacts-ico\">+1 (0) 000 0000 001</h5>\n                </div>\n                <div class=\"contacts-ico\">\n                    <span class=\"iconfont-wrapper\">\n                        <span mbr-icon=\"\" class=\"mbr-iconfont-c mbr-iconfont fa-map-marker fa\" mbr-if=\"showIcons\"></span>\n                    </span>\n                    <h5 class=\"mbr-fonts-style contacts\" mbr-theme-style=\"display-5\" data-app-selector=\".contacts-ico\">1234 Street Name\n                        <br>\n                    </h5>\n                </div>\n                <div class=\"contacts-ico\">\n                    <span class=\"iconfont-wrapper\">\n                        <span mbr-icon=\"\" class=\"mbr-iconfont-c mbr-iconfont fa-envelope-o fa\" mbr-if=\"showIcons\"></span>\n                    </span>\n                    <h5 class=\"mbr-fonts-style contacts\" mbr-theme-style=\"display-6\" data-app-selector=\".contacts-ico\">\n                        support@mobirise.com\n                    </h5>\n                </div>\n\n            </div>\n\n\n\n\n\n\n            <div class=\"mbr-col-lg-3 mbr-col-md-6 mbr-col-sm-12 padding\">\n                <h3 class=\"mbr-text mbr-bold mbr-fonts-style group-title mbr-white\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".group-title\">LATEST NEWS</h3>\n\n\n\n                <h5 class=\"mbr-text mbr-bold mbr-fonts-style sub-title\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".sub-title\">FITNESS CLASS FOR YOU</h5>\n                <p class=\"mbr-text text mbr-fonts-style\" mbr-theme-style=\"display-6\" data-app-selector=\".text\">OCTOBER 22, 2018</p>\n\n\n\n                <h5 class=\"mbr-text mbr-bold mbr-fonts-style sub-title\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".sub-title\">GYM EXERCISES</h5>\n                <p class=\"mbr-text text mbr-fonts-style\" mbr-theme-style=\"display-6\" data-app-selector=\".text\">NOVEMBER 22, 2018</p>\n\n\n\n                <h5 class=\"mbr-text mbr-bold mbr-fonts-style sub-title\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".sub-title\">BOOST PERFORMANCE</h5>\n                <p class=\"mbr-text text mbr-fonts-style\" mbr-theme-style=\"display-6\" data-app-selector=\".text\">NOVEMBER 17, 2018</p>\n\n\n            </div>\n\n\n\n\n            <div class=\"mbr-col-lg-3 mbr-col-md-6 mbr-col-sm-12 padding\" mbr-if=\"listItems!=0\">\n                <h3 class=\"mbr-text mbr-bold mbr-fonts-style group-title mbr-white\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".group-title\">PROJECTS</h3>\n                <div class=\"mbr-row items mbr-white\" mbr-list=\"\" mbr-list-grow=\"listItems\">\n                    <div class=\"list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                        <h5 class=\"mbr-fonts-style text2 mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".text2\">MARCH 2018</h5>\n                    </div>\n\n\n\n\n\n\n                    <div class=\"list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                        <h5 class=\"mbr-fonts-style text2 mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".text2\">FEBRUARY 2018</h5>\n                    </div>\n                    <div class=\"list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                        <h5 class=\"mbr-fonts-style text2 mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".text2\">JANUARY 2018</h5>\n                    </div>\n                    <div class=\"list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                        <h5 class=\"mbr-fonts-style text2 mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".text2\">DECEMBER 2018</h5>\n                    </div>\n                    <div class=\"list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                        <h5 class=\"mbr-fonts-style text2 mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".text2\">NOVEMBER 2018</h5>\n                    </div>\n                    <div class=\"list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12\">\n                        <h5 class=\"mbr-fonts-style text2 mbr-bold\" mbr-theme-style=\"display-7\" data-app-selector=\".text2\">OCTOBER 2018</h5>\n                    </div>\n                </div>\n            </div>\n\n\n\n            <div mbr-form=\"\" class=\"mbr-col-lg-3 mbr-col-md-12 mbr-col-sm-12 padding\">\n                <h3 class=\"mbr-text mbr-bold mbr-fonts-style group-title mbr-white\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".group-title\">\n                CONTACT</h3>\n                <form class=\"mbr-form\" action=\"\" method=\"post\" name=\"Mobirise Form\" target=\"_top\" novalidate=\"\">\n                    <div submit-success=\"\">\n                        <template data-form-alert=\"\" type=\"amp-mustache\">Subscription successful!</template>\n                    </div>\n                    <div submit-error=\"\">\n                        <template data-form-alert=\"\" type=\"amp-mustache\">Subscription successful!</template>\n                    </div>\n                    <div class=\"fieldset\">\n\n                        <input type=\"hidden\" name=\"form[data][1][0]\" value=\"Name\">\n                        <input class=\"input-label\" type=\"text\" name=\"form[data][1][1]\" data-form-field=\"Name\" placeholder=\"Name\" required=\"\">\n\n\n                        <input type=\"hidden\" name=\"form[data][2][0]\" value=\"Phone\">\n                        <input class=\"input-label\" type=\"phone\" name=\"form[data][2][1]\" data-form-field=\"Phone\" placeholder=\"Phone\" required=\"\" hidden=\"hidden\">\n\n\n                        <input type=\"hidden\" name=\"form[data][0][0]\" value=\"Email\">\n                        <input class=\"input-label\" type=\"email\" name=\"form[data][0][1]\" data-form-field=\"Email\" placeholder=\"Email\" required=\"\">\n\n                        <label class=\"area\">\n                            <input type=\"hidden\" name=\"form[data][3][0]\" value=\"Message\">\n                            <textarea name=\"form[data][3][1]\" rows=\"7\" data-form-field=\"Message\" placeholder=\"Message\"></textarea>\n                        </label>\n\n                    </div>\n\n                    <div mbr-buttons=\"\" mbr-theme-style=\"display-6\" data-toolbar=\"-mbrLink,-mbrBtnAdd,-mbrBtnMove\" class=\"mbr-section-btn\">\n                        <a type=\"submit\" class=\"btn btn-lg mbr-editable-button btn-primary\">\n                            <span class=\"btn-wrap\">SEND MESSAGE</span>\n                            <span class=\"iconfont-wrapper\">\n                                <span class=\"fa fa-arrow-right mbr-iconfont-btn mbr-iconfont\"></span>\n                            </span>\n                        </a>\n                    </div>\n\n                </form>\n\n\n            </div>\n\n        </div>\n    </div>\n</section>",
